    .when('/projects/new' , {
        templateUrl : 'assets/new_project.html',
        controller  : 'productCtrl',
        resolve:{
            loadProfile: checkLogin,
        }
    }).when('/projects' , {
        templateUrl : 'assets/project_index.html',
        controller  : 'projectCtrl',
        resolve: {
            loadProfile: checkRouting,
        }
    })
})

